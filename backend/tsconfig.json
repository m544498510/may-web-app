{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "strict": true,
    "sourceMap": true,
    "removeComments": true,
    "outDir": "dist/",
    "lib": [
      "es2015",
      "es6",
      "esnext.asynciterable"
    ],
    "baseUrl": "src",
    "paths": {
      "utils/*": [ "utils/*" ]
    }

  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
	  "node_modules"
  ]
}
